    <sql>SELECT
  tu.IDENTIFIER
, tu.EMAIL_ADDRESS
, tu.FIRST_NAME
, tu.LAST_NAME
, tu.LOGIN_NAME
, tu.START_DATE
, tu.ACTIVITY_STATUS
, tu.DEPARTMENT_ID
, tu.CANCER_RESEARCH_GROUP_ID
, tu.INSTITUTION_ID
, tu.ADDRESS_ID
, tu.CSM_USER_ID
, tu.STATUS_COMMENT
, tu.FIRST_TIME_LOGIN
, cu.phone_number
FROM catissue_user tu, csm_user cu
where tu.identifier in
(select p.PRINCIPAL_INVESTIGATOR_ID from  catissue_specimen_protocol p)
and cu.user_id = tu.identifier
</sql>
